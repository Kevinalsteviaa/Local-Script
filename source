local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
getgenv().SecureMode = true


local Window = Rayfield:CreateWindow({
	Name = "Local Script",
	LoadingTitle = "Local Script, Some Scripts Are Collected To Make It Easier",
	LoadingSubtitle = "By VinTM",
})

--Script
local TabScript = Window:CreateTab("Script")
local Label = TabScript:CreateLabel("Thanks for using this Script, Enjoy")
local Section = TabScript:CreateSection("Some Script :")
--Credit
local TabCredit = Window:CreateTab("Credit")
local SectionUtil = TabCredit:CreateSection("Credit")
local Paragraph = TabCredit:CreateParagraph({Title = "Crate This Script :", Content = "VinTM"})
local Paragraph = TabCredit:CreateParagraph({Title = "User Name Roblox :", Content = "Kevinalsteviaa"})
local SectionUtil = TabCredit:CreateSection("")
local Paragraph = TabCredit:CreateParagraph({Title = "Helper Name :", Content = "There isn't any ;-;"})
--Setting
local TabSetting = Window:CreateTab("Setting")
local SectionUtil = TabSetting:CreateSection("Change Keybind")
local ButtonDestroy = TabSetting:CreateButton({
	Name = "Close ( Is Make This UI Destroy )",
	Callback = function()
		Rayfield:Destroy()
	end,
}) --Close

--Value
local Button = TabScript:CreateButton({
